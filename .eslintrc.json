{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "brace-style": "off",
      "class-methods-use-this": "warn",
      "global-require": "off",
      "new-cap": "off",
      "no-bitwise": "off",
      "no-console": "off",
      "no-nested-ternary": "off",
      "no-plusplus": "off",
      "no-prototype-builtins": "off",
      "object-curly-newline": "off",
      "one-var": "off",
      "one-var-declaration-per-line": "off"
    }
}
