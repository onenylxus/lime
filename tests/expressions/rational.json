{
  "name": "Rational expression test",
  "version": 2,
  "process": [
    {
      "desc": "natural define",
      "config": {},
      "tests": [
        {
          "input": ["1/2"],
          "type": "equals",
          "output": "1/2"
        },
        {
          "input": ["1/-2"],
          "type": "equals",
          "output": "-1/2"
        }
      ]
    },
    {
      "desc": "decimal display",
      "config": {
        "decimalPlaces": 4
      },
      "tests": [
        {
          "input": ["1/3"],
          "type": "equals",
          "output": "0.3333"
        }
      ]
    }
  ]
}
