      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false%true"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["true%true"],
          "type": "equals",
          "output": "0"
        }
      ]
    },
    {
      "desc": "boolean with integer",
      "config": {
        "strictBoolean": false
      },
      "tests": [
