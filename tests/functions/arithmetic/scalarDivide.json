{
  "name": "Scalar divide function test",
  "version": 2,
  "process": [
    {
      "desc": "complex with matrix",
      "config": {},
      "tests": [
        {
          "input": ["i./[1,2;3,4]"],
          "type": "equals",
          "output": "[i,1/2i;1/3i,1/4i]"
        },
        {
          "input": ["[1,2;3,4]./i"],
          "type": "equals",
          "output": "[-i,-2i;-3i,-4i]"
        }
      ]
    },
    {
      "desc": "integer with matrix",
      "config": {},
      "tests": [
        {
          "input": ["3./[1,2;3,4]"],
          "type": "equals",
          "output": "[3,3/2;1,3/4]"
        },
        {
          "input": ["[-1,0;3,-2]./2"],
          "type": "equals",
          "output": "[-1/2,0;3/2,-1]"
        }
      ]
    },
    {
      "desc": "matrix with matrix",
      "config": {},
      "tests": [
        {
          "input": ["[1,6]./[-1,3]"],
          "type": "equals",
          "output": "[-1,2]"
        }
      ]
    },
    {
      "desc": "matrix with rational",
      "config": {},
      "tests": [
        {
          "input": ["[1,2;3,4]./(1/2)"],
          "type": "equals",
          "output": "[2,4;6,8]"
        },
        {
          "input": ["(1/5)./[1,2;3,4]"],
          "type": "equals",
          "output": "[1/5,1/10;1/15,1/20]"
        }
      ]
    }
  ]
}
