{
  "name": "Scalar modulo function test",
  "version": 2,
  "process": [
    {
      "desc": "integer with matrix",
      "config": {},
      "tests": [
        {
          "input": ["7.%[1,2;3,4]"],
          "type": "equals",
          "output": "[0,1;1,3]"
        },
        {
          "input": ["[1,2;3,4].%3"],
          "type": "equals",
          "output": "[1,2;0,1]"
        }
      ]
    },
    {
      "desc": "matrix with matrix",
      "config": {},
      "tests": [
        {
          "input": ["[16,17].%[5,6]"],
          "type": "equals",
          "output": "[1,5]"
        },
        {
          "input": ["[1,2].%[1;3]"],
          "type": "throws",
          "output": "error:invalidMatrixDimensions"
        }
      ]
    }
  ]
}
