{
  "name": "Scalar multiply function test",
  "process": [
    {
      "desc": "complex with matrix",
      "config": {},
      "tests": [
        ["i.*[1,2;3,4]", "[i,2i;3i,4i]"],
        ["[1,2;3,4].*i", "[i,2i;3i,4i]"]
      ]
    },
    {
      "desc": "integer with matrix",
      "config": {},
      "tests": [
        ["3.*[1,2;3,4]", "[3,6;9,12]"],
        ["[-1,0;3,-2].*2", "[-2,0;6,-4]"]
      ]
    },
    {
      "desc": "matrix with matrix",
      "config": {},
      "tests": [
        ["[1,0].*[-1,3]", "[-1,0]"]
      ]
    },
    {
      "desc": "matrix with rational",
      "config": {},
      "tests": [
        ["[1,2;3,4].*(1/2)", "[1/2,1;3/2,2]"],
        ["(1/5).*[1,2;3,4]", "[1/5,2/5;3/5,4/5]"]
      ]
    }
  ]
}
