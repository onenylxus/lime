{
  "name": "Subtract function test",
  "version": 2,
  "process": [
    {
      "desc": "boolean with boolean",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false-false"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["false-true"],
          "type": "equals",
          "output": "-1"
        },
        {
          "input": ["true-false"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["true-true"],
          "type": "equals",
          "output": "0"
        }
      ]
    },
    {
      "desc": "boolean with complex",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false-(1+2i)"],
          "type": "equals",
          "output": "-1-2i"
        },
        {
          "input": ["(6-i)-true"],
          "type": "equals",
          "output": "5-i"
        }
      ]
    },
    {
      "desc": "boolean with integer",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false-6"],
          "type": "equals",
          "output": "-6"
        },
        {
          "input": ["2-true"],
          "type": "equals",
          "output": "1"
        }
      ]
    },
    {
      "desc": "boolean with rational",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["true-(5/7)"],
          "type": "equals",
          "output": "2/7"
        },
        {
          "input": ["(1/3)-false"],
          "type": "equals",
          "output": "1/3"
        }
      ]
    },
    {
      "desc": "complex with complex",
      "config": {},
      "tests": [
        {
          "input": ["(1+4i)-(2-5i)"],
          "type": "equals",
          "output": "-1+9i"
        }
      ]
    },
    {
      "desc": "complex with integer",
      "config": {},
      "tests": [
        {
          "input": ["(1+4i)-3"],
          "type": "equals",
          "output": "-2+4i"
        },
        {
          "input": ["5-(-3+3i)"],
          "type": "equals",
          "output": "8-3i"
        }
      ]
    },
    {
      "desc": "complex with rational",
      "config": {},
      "tests": [
        {
          "input": ["(1-2i)-(1/2)"],
          "type": "equals",
          "output": "1/2-2i"
        },
        {
          "input": ["(4/3)-(2/3+i)"],
          "type": "equals",
          "output": "2/3-i"
        }
      ]
    },
    {
      "desc": "integer with integer",
      "config": {},
      "tests": [
        {
          "input": ["3-2"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["2-3"],
          "type": "equals",
          "output": "-1"
        }
      ]
    },
    {
      "desc": "integer with rational",
      "config": {},
      "tests": [
        {
          "input": ["1-(1/4)"],
          "type": "equals",
          "output": "3/4"
        },
        {
          "input": ["(3/4)-2"],
          "type": "equals",
          "output": "-5/4"
        }
      ]
    },
    {
      "desc": "rational with rational",
      "config": {},
      "tests": [
        {
          "input": ["(1/3)-(1/4)"],
          "type": "equals",
          "output": "1/12"
        },
        {
          "input": ["(1/4)-(1/3)"],
          "type": "equals",
          "output": "-1/12"
        }
      ]
    },
    {
      "desc": "matrix with matrix",
      "config": {},
      "tests": [
        {
          "input": ["[1,6,0;3,2,1]-[-2,5,-1;3,1,4]"],
          "type": "equals",
          "output": "[3,1,1;0,1,-3]"
        }
      ]
    }
  ]
}
