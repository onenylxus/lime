{
  "name": "Bitwise NOT function test",
  "version": 2,
  "process": [
    {
      "desc": "boolean",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["~false"],
          "type": "equals",
          "output": "-1"
        },
        {
          "input": ["~true"],
          "type": "equals",
          "output": "-2"
        }
      ]
    },
    {
      "desc": "integer",
      "config": {},
      "tests": [
        {
          "input": ["~5"],
          "type": "equals",
          "output": "-6"
        },
        {
          "input": ["~-2"],
          "type": "equals",
          "output": "1"
        }
      ]
    }
  ]
}
