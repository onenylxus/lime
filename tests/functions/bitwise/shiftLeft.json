{
  "name": "Shift left function test",
  "version": 2,
  "process": [
    {
      "desc": "boolean with boolean",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false<<false"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["false<<true"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["true<<false"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["true<<true"],
          "type": "equals",
          "output": "2"
        }
      ]
    },
    {
      "desc": "boolean with integer",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false<<2"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["5<<true"],
          "type": "equals",
          "output": "10"
        }
      ]
    },
    {
      "desc": "integer with integer",
      "config": {},
      "tests": [
        {
          "input": ["5<<1"],
          "type": "equals",
          "output": "10"
        }
      ]
    }
  ]
}
