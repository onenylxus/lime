{
  "name": "Smaller function test",
  "version": 2,
  "process": [
    {
      "desc": "boolean with boolean",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false<false"],
          "type": "equals",
          "output": "false"
        },
        {
          "input": ["false<true"],
          "type": "equals",
          "output": "true"
        },
        {
          "input": ["true<false"],
          "type": "equals",
          "output": "false"
        },
        {
          "input": ["true<true"],
          "type": "equals",
          "output": "false"
        }
      ]
    },
    {
      "desc": "boolean with integer",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false<3"],
          "type": "equals",
          "output": "true"
        },
        {
          "input": ["4<true"],
          "type": "equals",
          "output": "false"
        }
      ]
    },
    {
      "desc": "boolean with rational",
      "config": {
        "strictBoolean": false
      },
      "tests": [
        {
          "input": ["false<(1/6)"],
          "type": "equals",
          "output": "true"
        },
        {
          "input": ["(8/3)<true"],
          "type": "equals",
          "output": "false"
        }
      ]
    },
    {
      "desc": "integer with integer",
      "config": {},
      "tests": [
        {
          "input": ["1<1"],
          "type": "equals",
          "output": "false"
        },
        {
          "input": ["1<2"],
          "type": "equals",
          "output": "true"
        },
        {
          "input": ["2<1"],
          "type": "equals",
          "output": "false"
        }
      ]
    },
    {
      "desc": "integer with rational",
      "config": {},
      "tests": [
        {
          "input": ["1<(1/2)"],
          "type": "equals",
          "output": "false"
        },
        {
          "input": ["(1/2)<1"],
          "type": "equals",
          "output": "true"
        }
      ]
    },
    {
      "desc": "rational with rational",
      "config": {},
      "tests": [
        {
          "input": ["(1/2)<(1/3)"],
          "type": "equals",
          "output": "false"
        }
      ]
    }
  ]
}
