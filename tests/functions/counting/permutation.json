{
  "name": "Permutation function test",
  "version": 2,
  "process": [
    {
      "desc": "argument of integer and integer",
      "config": {},
      "tests": [
        {
          "input": ["npr(3,2)"],
          "type": "equals",
          "output": "6"
        },
        {
          "input": ["npr(3,4)"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["npr(-1,2)"],
          "type": "throws",
          "output": "warn:negativePermutation"
        },
        {
          "input": ["npr(2,-1)"],
          "type": "throws",
          "output": "warn:negativePermutation"
        }
      ]
    }
  ]
}
