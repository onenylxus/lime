          "output": "false"
        }
      ]
    },
    {
      "desc": "integer",
      "config": {},
      "tests": [
        {
          "input": ["!0"],
          "type": "equals",
          "output": "true"
        },
        {
          "input": ["!2"],
          "type": "equals",
          "output": "false"
        }
      ]
    },
    {
      "desc": "rational",
      "config": {},
      "tests": [
        {
          "input": ["!(1/2)"],
          "type": "equals",
          "output": "false"
        }
      ]
    }
  ]
}
