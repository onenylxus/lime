{
  "name": "Inverse function test",
  "version": 2,
  "process": [
    {
      "desc": "argument of matrix",
      "config": {},
      "tests": [
        {
          "input": ["inv([1,2;3,4])"],
          "type": "equals",
          "output": "[-2,1;3/2,-1/2]"
        },
        {
          "input": ["A=[1,2;3,4]", "A*inv(A)"],
          "type": "equals",
          "output": "[1,0;0,1]"
        },
        {
          "input": ["A=[1,2;3,4]", "inv(A)*A"],
          "type": "equals",
          "output": "[1,0;0,1]"
        },
        {
          "input": ["inv([1,1;1,1])"],
          "type": "throws",
          "output": "error:zeroMatrixDeterminant"
        }
      ]
    }
  ]
}
