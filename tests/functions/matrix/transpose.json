{
  "name": "Transpose function test",
  "version": 2,
  "process": [
    {
      "desc": "argument of matrix",
      "config": {},
      "tests": [
        {
          "input": ["transpose([1])"],
          "type": "equals",
          "output": "[1]"
        },
        {
          "input": ["transpose([1,2;3,4])"],
          "type": "equals",
          "output": "[1,3;2,4]"
        },
        {
          "input": ["transpose([1,2,3;4,5,6])"],
          "type": "equals",
          "output": "[1,4;2,5;3,6]"
        }
      ]
    }
  ]
}
