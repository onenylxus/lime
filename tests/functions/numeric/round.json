{
  "name": "Round function test",
  "version": 2,
  "process": [
    {
      "desc": "argument of integer",
      "config": {},
      "tests": [
        {
          "input": ["round(5)"],
          "type": "equals",
          "output": "5"
        }
      ]
    },
    {
      "desc": "argument of rational",
      "config": {},
      "tests": [
        {
          "input": ["round(5/3)"],
          "type": "equals",
          "output": "2"
        },
        {
          "input": ["round(4/3)"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["round(-5/3)"],
          "type": "equals",
          "output": "-2"
        }
      ]
    }
  ]
}
