{
  "name": "Sign function test",
  "version": 2,
  "process": [
    {
      "desc": "argument of integer",
      "config": {},
      "tests": [
        {
          "input": ["sgn(3)"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["sgn(0)"],
          "type": "equals",
          "output": "0"
        },
        {
          "input": ["sgn(-3)"],
          "type": "equals",
          "output": "-1"
        }
      ]
    },
    {
      "desc": "argument of rational",
      "config": {},
      "tests": [
        {
          "input": ["sgn(5/2)"],
          "type": "equals",
          "output": "1"
        },
        {
          "input": ["sgn(-5/2)"],
          "type": "equals",
          "output": "-1"
        }
      ]
    }
  ]
}
